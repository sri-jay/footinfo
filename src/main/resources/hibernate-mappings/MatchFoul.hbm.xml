<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.abyeti.footinfo.db.ObjectMappings.MatchFoul" table="match_foul">
        <meta attribute="class-description">
            Class that holds fouls commiteed during a match.
        </meta>

        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>

        <property name="commitedBy" column="commited_by" type="text"/>
        <property name="foulOn" column="foul_on" type="text"/>
        <property name="foulTime" column="foul_time" type="text"/>
        <property name="matchId" column="match_id" type="text"/>
    </class>
</hibernate-mapping>