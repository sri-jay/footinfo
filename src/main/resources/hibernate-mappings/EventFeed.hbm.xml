<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.abyeti.footinfo.db.ObjectMappings.EventFeed" table="event_feed">
        <meta attribute="class-description">
            Event Feed from which front end info can be extracted.
        </meta>

        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>

        <property name="matchId" column="match_id" type="text"/>
        <property name="eventText" column="event_text" type="text"/>
    </class>
</hibernate-mapping>